{
  "name": "e2design/craft-boilerplate",
  "description": "A starter Craft install for all future E2design projects",
  "version": "1.0.4",
  "type": "project",
  "require": {
    "barrelstrength/sprout-encode-email": "2.0.7",
    "barrelstrength/sprout-fields": "^3.8.3",
    "craftcms/aws-s3": "^1.2.11",
    "craftcms/cms": "3.5.17.1",
    "craftcms/redactor": "2.8.5",
    "hopstudios/hop-reveal": "1.0.0",
    "mmikkel/cp-clearcache": "^1.0.7",
    "nystudio107/craft-fastcgicachebust": "1.0.9",
    "nystudio107/craft-minify": "^1.2.10",
    "nystudio107/craft-typogrify": "^1.1.18",
    "sebastianlenz/linkfield": "1.0.24",
    "servd/craft-asset-storage": "2.0.3",
    "spicyweb/craft-neo": "2.8.15.1",
    "superbig/craft3-http2serverpush": "^1.0.1",
    "verbb/cp-nav": "3.0.13.1",
    "verbb/super-table": "2.6.5",
    "vlucas/phpdotenv": "^3.4.0",
    "yiisoft/yii2-redis": "^2.0.6"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
